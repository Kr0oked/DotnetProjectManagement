{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route.project.web-api": {
        "ClusterId": "cluster.project.web-api",
        "Match": {
          "Path": "/api/project/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/project"
          }
        ]
      }
    },
    "Clusters": {
      "cluster.project.web-api": {
        "Destinations": {
          "destination.default": {
            "Address": "http://project-web-api"
          }
        }
      }
    }
  }
}
